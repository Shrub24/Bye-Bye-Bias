{
	"name": "Bye Bye Bias",
	"version": "6.9",
	"description": "Combatting confirmation bias one article at a time",
	"permissions": ["tabs", "activeTab", "identity", "identity.email"],
    "browser_action": {
      "default_icon": {                    // optional
        "32": "./icon32.png",           // optional
        "64": "./icon64.png",           // optional
        "128": "./icon128.png"            // optional
      },
      "default_popup": "popup.html"
    },
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"manifest_version": 2
}