{
	"name": "Bye Bye Bias",
	"version": "1.0",
	"description": "Combatting confirmation bias one article at a time",
	"permissions": ["storage", "activeTab"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"manifest_version": 2,
	"content_scripts": [
	  {
	    "matches": [
	      "<all_urls>"
	    ],
	    "js": ["content.js"]
	  }
	]
}